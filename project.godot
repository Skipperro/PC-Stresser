; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="PC Stresser"
run/main_scene="res://Stresser.tscn"
boot_splash/image="res://fire.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://fire.png"
config/windows_native_icon="res://fire.ico"

[autoload]

ReflectMaterialManager="*res://addons/Silicon.vfx.planar_reflection/reflect_material_manager.gd"

[debug]

settings/fps/force_fps=240
settings/stdout/print_fps=true

[display]

window/size/width=512
window/size/height=512
window/size/resizable=false
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false

[editor_plugins]

enabled=PoolStringArray( "Silicon.vfx.planar_reflection" )

[rendering]

threads/thread_model=2
quality/filters/anisotropic_filter_level=16
quality/shadows/filter_mode=2
quality/subsurface_scattering/quality=2
quality/subsurface_scattering/follow_surface=true
quality/voxel_cone_tracing/high_quality=true
quality/filters/msaa=4
environment/default_environment="res://default_env.tres"
